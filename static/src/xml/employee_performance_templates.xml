            <div class="row" t-if="values['marketing_data']">
                <div class="container-fluid">
                    <div class="row justify-content-center">
                        <div class="col m-2">
                            <div class="container-fluid mt-3">
                                <div>
                                    <div class="container-fluid mt-3">
                                        <div class="row justify-content-center">
                                            <div class="col">                            
                                                <div class="districtchart-canvas-container"
                                                     style="position:relative;height:500px;">
                                                    <canvas id="districtWiseLeadChart"
                                                            style="width:700px;height:450px;"></canvas>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row m-4" t-if="values['academic_data']">
                <div class="employee_details_card academic_batch_data" style="width:100%;height:auto;">
                    <div class="card-header row px-0 mr-0 ml-0" style="border:0px">
                        
                        <div t-if="values['academic_data']" class="mr-0 ml-2 p-0">
                            <select class="academic_window form-control mr-2 font-weight-bold"
                                    style="display: inline-block; width: auto;background-color:#edf0f2;">
                                <t t-if="values['academic_data']['academic_windows']"
                                   t-foreach="values['academic_data']['academic_windows']" t-as="academic_window">
                                    <option t-att-value="academic_window['id']" t-esc="academic_window['name']"/>
                                </t>
                            </select>
                        </div>

                        <div class="academic_batches_list">
                            <!-- <div t-if="values['academic_data']" class="col">
                                <select class="academic_batch form-control mr-2" style="display: inline-block; width: auto;background-color:#bfc1c2;">
                                    <t t-if="values['academic_data']['batches']" t-foreach="values['academic_data']['batches']" t-as="batch">
                                        <option t-att-value="batch['id']" t-esc="batch['name'] + ' (' + batch['strength'] + ' Students)'"/>
                                    </t>
                                </select>
                            </div> -->
                        </div>

                        <div class="col">
                            <h2>Batch Summary</h2>

                        </div>
                    </div>
                </div>
            </div>


            <div class="row" t-if="values['sales_data']">
            <!-- <div class="row" t-if="values['sales_data']"> -->
                <div class="col container-fluid">
                        <div class="row justify-content-start">
                            <div class="col m-2">
                                <div class="container-fluid mt-3">
                                    <div>
                                        <div class="container-fluid mt-3">
                                            <div class="row justify-content-center">
                                                <div class="col">                            
                                                    <div class="sourcechart-canvas-container"
                                                         style="position:relative;height:500px;">
                                                        <canvas id="sourceWiseLeadChart"
                                                                style="width:700px;height:450px;"></canvas>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
            </div>
            <!-- </div> -->

            <!-- <div class="row" t-if="values['sales_data']"> -->
            <div class="row" t-if="values['sales_data']">
                    <div class="col container-fluid">
                        <div class="row justify-content-start">
                            <div class="col m-2">
                                <div class="container-fluid mt-3">
                                    <div>
                                        <div class="container-fluid mt-3">
                                            <div class="row justify-content-center">
                                                <div class="col">                            
                                                    <div class="coursechart-canvas-container"
                                                         style="position:relative;height:500px;">
                                                        <canvas id="courseWiseLeadChart"
                                                                style="width:700px;height:450px;"></canvas>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
            </div>

            <div class="row m-2">
                <t t-if="values['common_performance']">
                    <div class="col-md-2 col-sm-6 oh-payslip">
                        <div class="oh-card model_record_card" id="logic.task.other" name="Miscellaneous Works">
                            <div class="oh-card-body">
                                <div class="stat-widget-one">
                                    <div class="stat-icon "><img class="img-fluid" src="/logic_performance_tracker/static/src/icons/miscellaneous.png"/></div>
                                    <div class="stat-content">
                                        <div class="stat-text">Miscellaneous</div>
                                        <div class="stat-digit"><t t-esc="values['common_performance']['misc_task_count']"/></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-2 col-sm-6 oh-payslip">
                        <div class="oh-card model_record_card" id="to_do.tasks" name="To Do">
                            <div class="oh-card-body">
                                <div class="stat-widget-one">
                                    <div class="stat-icon "><img class="img-fluid" src="/logic_performance_tracker/static/src/icons/to_do.png"/></div>
                                    <div class="stat-content">
                                        <div class="stat-text">To Do</div>
                                        <div class="stat-digit"><t t-esc="values['common_performance']['to_do_count']"/></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-2 col-sm-6 oh-payslip">
                        <div class="oh-card model_record_card" id="directors.feedback" name="Directors Feedback">
                            <div class="oh-card-body">
                                <div class="stat-widget-one">
                                    <div class="stat-icon "><img class="img-fluid" src="/logic_performance_tracker/static/src/icons/feedback.png"/></div>
                                    <div class="stat-content">
                                        <div class="stat-text">FeedBacks</div>
                                        <div class="stat-digit"><t t-esc="values['common_performance']['common_feedbacks']"/></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </t>

                <t t-if="values['digital_data']">
                    <div class="col-md-2 col-sm-6 oh-payslip">
                        <div class="oh-card model_record_card" id="digital.task" name="Digital Tasks">
                            <div class="oh-card-body">
                                <div class="stat-widget-one">
                                    <div class="stat-icon "><img class="img-fluid" src="/logic_performance_tracker/static/src/icons/digital.png"/></div>
                                    <div class="stat-content">
                                        <div class="stat-text">Digital</div>
                                        <div class="stat-digit"><t t-esc="values['digital_data']['digital_task_count']"/></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </t>

                <t t-if="values['marketing_data']">
                    <div class="col-md-2 col-sm-6 oh-payslip">
                        <div class="oh-card model_record_card" id="seminar.leads" name="Seminar">
                            <div class="oh-card-body">
                                <div class="stat-widget-one">
                                    <div class="stat-icon "><img class="img-fluid" src="/logic_performance_tracker/static/src/icons/seminar.png"/></div>
                                    <div class="stat-content">
                                        <div class="stat-text">Seminar</div>
                                        <div class="stat-digit"><t t-esc="values['marketing_data']['leads_count']"/></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </t>

                <t t-if="values['sales_data']">
                    <div class="col-md-2 col-sm-6 oh-payslip">
                        <div class="oh-card model_record_card" id="leads.logic" name="Leads">
                            <div class="oh-card-body">
                                <div class="stat-widget-one">
                                    <div class="stat-icon "><img class="img-fluid" src="/logic_performance_tracker/static/src/icons/leads.png"/></div>
                                    <div class="stat-content">
                                        <div class="stat-text">Leads</div>
                                        <div class="stat-digit"><t t-esc="values['sales_data']['leads_count']"/></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </t>

                <t t-if="values['academic_data']">
                    <t t-if="!values['academic_data']['is_associate_faculty']">
                        <div class="col-md-2 col-sm-6 oh-payslip">
                            <div class="oh-card model_record_card" id="upaya.form" name="Upaya">
                                <div class="oh-card-body">
                                    <div class="stat-widget-one">
                                        <div class="stat-icon "><img class="img-fluid" src="/logic_performance_tracker/static/src/icons/upaya.png"/></div>
                                        <div class="stat-content">
                                            <div class="stat-text">Upaya</div>
                                            <div class="stat-digit"><t t-esc="values['academic_data']['upaya_count']"/></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-2 col-sm-6 oh-payslip">
                            <div class="oh-card model_record_card" id="yes_plus.logic" name="Yes Plus">
                                <div class="oh-card-body">
                                    <div class="stat-widget-one">
                                        <div class="stat-icon"><img class="overflow-auto img-fluid" src="/logic_performance_tracker/static/src/icons/yes_plus.png"/></div>
                                        <div class="stat-content">
                                            <div class="stat-text">Yes Plus</div>
                                            <div class="stat-digit"><t t-esc="values['academic_data']['yes_plus_count']"/></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-2 col-sm-6 oh-payslip">
                            <div class="oh-card model_record_card" id="student.faculty" name="Student Faculty Club">
                                <div class="oh-card-body">
                                    <div class="stat-widget-one">
                                        <div class="stat-icon"><img class="overflow-auto img-fluid" src="/logic_performance_tracker/static/src/icons/sfc.png"/></div>
                                        <div class="stat-content">
                                            <div class="stat-text">SFC</div>
                                            <div class="stat-digit"><t t-esc="values['academic_data']['sfc_count']"/></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-2 col-sm-6 oh-payslip">
                            <div class="oh-card model_record_card" id="one_to_one.meeting" name="One to One">
                                <div class="oh-card-body">
                                    <div class="stat-widget-one">
                                        <div class="stat-icon"><img class="overflow-auto img-fluid" src="/logic_performance_tracker/static/src/icons/one_to_one.png"/></div>
                                        <div class="stat-content">
                                            <div class="stat-text">One To One</div>
                                            <div class="stat-digit"><t t-esc="values['academic_data']['one2one_count']"/></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-2 col-sm-6 oh-payslip">
                            <div class="oh-card model_record_card" id="logic.mock_interview" name="Mock Interview">
                                <div class="oh-card-body">
                                    <div class="stat-widget-one">
                                        <div class="stat-icon"><img class="overflow-auto img-fluid" src="/logic_performance_tracker/static/src/icons/mock_interview.png"/></div>
