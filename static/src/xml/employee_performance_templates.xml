<?xml version="1.0" encoding="UTF-8"?>

<templates xml:space="preserve">
    <!-- Define your QWeb template for the dashboard_card view -->
    <t t-name="logic_performance_tracker.employee_performance_template">
        <link rel="stylesheet" type="text/css"
              href="/logic_performance_tracker/static/src/css/dashboard_card_view.css"/>
        <link rel="stylesheet" type="text/css"
              href="/logic_performance_tracker/static/src/css/lib/jquery.orgchart.css"/>
        <!-- <link rel="stylesheet" type="text/css" href="/logic_performance_tracker/static/src/css/lib/jquery-ui.css"/> -->

        <link rel="stylesheet" type="text/css" href="/logic_performance_tracker/static/src/css/orgchart_styles.css"/>
        <link rel="stylesheet" type="text/css" href="/logic_performance_tracker/static/src/css/other_styles.css"/>
        <link rel="stylesheet" type="text/css" href="/logic_performance_tracker/static/src/css/progressbar.css"/>

        <link rel="stylesheet" type="text/css"
              href="/logic_performance_tracker/static/src/css/employee_performance_styles.css"/>

        <style>
            <!-- .row{
                margin-right:0;
                margin-left:0;
            } -->
            .badge-secondary{
            background-color: #CAD6D1;
            }
        </style>
        <div t-if="values" class="container-fluid employee_performance" style="margin:0;padding:0;">
            <div class="row">
                <div class="container-fluid mt-3 mr-0 ml-0 p-0">
                    <div class="row justify-content-center">
                        <div class="col-auto ml-5" style="">   
                            <!-- <span class="font-weight-bold">Employee </span> -->
                            <select class="department_employee form-control mr-2 font-weight-bold"
                                    style="display: inline-block; width: auto;background-color:#edf0f2;">
                                <option t-if="values['personal_data']" t-att-value="values['personal_data']['id']">
                                    <div>
                                        <span t-esc="values['personal_data']['name']"/>
                                    </div>
                                </option>

                                <t t-if="values['department_employees']" t-foreach="values['department_employees']"
                                   t-as="department_employee">
                                    <option t-if="department_employee" t-att-value="department_employee['id']">
                                        <div>
                                            <span t-esc="department_employee['name']"/>
                                        </div>
                                    </option>
                                </t>
                            </select> 
                        </div>
                        <div class="col-auto">
                            <button class="form-control btn-primary report_download_btn">Download</button>
                        </div>
                        <div class="col-auto" style="margin-left: auto; margin-right: 0;">   
                            <span class="font-weight-bold">From: </span>
                            <input class="from_date date-picker-icon custom-date-input" type="date"/>
                            <span class="font-weight-bold"> To: </span>
                            <input placeholder="From: " class="end_date date-picker-icon custom-date-input"
                                   type="date"/>
                            <button class="ml-2 filter-button btn-md o_filter_performance" type="button">Filter</button>
                            <button class="rounded btn-md btn-warning o_filter_reset" type="button">Reset</button>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="container-fluid">
                    <div class="row justify-content-start">
                        <div class="col-4">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col">
                                        <div t-if="values['personal_data']" class="employee_details_card card mt-3">
                                            <div class="card-body">
                                                <div class="row">
                                                    <div class="col">

                                                        <div class="personal-details">
                                                            <div class="text-center">
                                                                <img t-att-src="values['personal_data']['image']"
                                                                     class="m-3"
                                                                     style="height: 100px; width:auto; border-radius:20px;"/>
                                                            </div>
                                                            <div class="detail-item row mb-1">
                                                                <div class="col-4">
                                                                    <span class="label1 font-weight-normal badge badge-secondary">Name</span>
                                                                </div>
                                                                <div class="col-8">
                                                                        <span class="value font-weight-bold"
                                                                              t-esc="values['personal_data']['name']"/>

                                                                </div>
                                                            </div>
                                                            <div class="detail-item row mb-1">
                                                                <div class="col-4">
                                                                    <span class="label1 badge badge-info">Position</span>

                                                                </div>
                                                                <div class="col-8">
                                                                    <!-- <span class="label1 font-weight-normal">: </span> -->

                                                                    <span class="value font-weight-bold"
                                                                          t-esc="values['personal_data']['job_title']"/>

                                                                </div>
                                                            </div>
                                                            <div class="detail-item row mb-1">
                                                                <div class="col-4">
                                                                    <span class="label1 badge badge-primary">Department</span>

                                                                </div>
                                                                <div class="col-8">

                                                                    <span class="value font-weight-bold"
                                                                          t-esc="values['personal_data']['department_name']"/>
                                                                </div>
                                                            </div>
                                                            <div class="detail-item row mb-1"
                                                                 t-if="values['personal_data']['branch']">
                                                                <div class="col-4">
                                                                    <span class="label1 badge badge-warning">Branch</span>
                                                                </div>
                                                                <div class="col-8">
                                                                    <!-- <span class="label1 font-weight-normal">: </span> -->

                                                                    <span class="value font-weight-bold"
                                                                          t-esc="values['personal_data']['branch']"/>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col">
                                        <div class="employee_details_card card mt-3" style="height: 100%;">
                                            <div class="card-body">
                                                <h2 class="card-title"><u>Performance Details</u></h2>

                                                <div class="detail-item row" t-if="values['common_performance']">
                                                    <div class="col-7">
                                                        <span class="label1 text-nowrap">Miscellaneous Tasks</span>
                                                    </div>
                                                    <div class="col-5 ml-0 mr-0 p-0">
                                                        <span class="label1 font-weight-normal">: </span>

                                                        <span class="number-value"
                                                              t-esc="values['common_performance']['completed_misc_count']"/>
                                                        <span class=""
                                                              t-esc="' ('+values['common_performance']['delayed_misc_count'] + ' Delayed )'"/>
                                                    </div>

                                                </div>
                                                <div class="detail-item row" t-if="values['common_performance']">
                                                    <div class="col-7">

                                                        <span class="label1">To Do Tasks:</span>
                                                    </div>
                                                    <div class="col-5 ml-0 mr-0 p-0">
                                                        <span class="label1 font-weight-normal">: </span>

                                                        <span class="number-value"
                                                              t-esc="values['common_performance']['to_do_count']"/>
                                                        <span class=""
                                                              t-esc="' ( '+values['common_performance']['delayed_to_do_count'] + ' Delayed )'"/>
                                                    </div>

                                                </div>
                                                <div class="detail-item row" t-if="values['marketing_data']">
                                                    <div class="col-7">
                                                        <span class="label1">Seminar Target:</span>
                                                    </div>
                                                    <div class="col-5 ml-0 mr-0 p-0">
                                                        <span class="label1 font-weight-normal">: </span>

                                                        <span class="number-value"
                                                              t-esc="values['marketing_data']['year_leads_count']"/>
                                                        <span>/</span>
                                                        <span class="number-value text-primary"
                                                              t-esc="values['marketing_data']['year_leads_target']"/>
                                                        <span class="font-weight-bold" t-esc="' ('+values['year']+')'"/>
                                                    </div>
                                                </div>

                                                <div class="detail-item row" t-if="values['sales_data']">
                                                    <div class="col-7">
                                                        <span class="label1">Admission Target:</span>
                                                    </div>
                                                    <div class="col-5 ml-0 mr-0 p-0 text-nowrap">
                                                        <span class="label1 font-weight-normal">: </span>

                                                        <span class="number-value"
                                                              t-esc="values['sales_data']['month_year_converted_leads_count']"/>
                                                        <span>/</span>
                                                        <span class="number-value text-primary"
                                                              t-esc="values['sales_data']['month_year_leads_target']"/>
                                                        <span t-if="values['month']" class="font-weight-bold"
                                                              t-esc="' ('+values['month']+ ' '+values['year']+')'"/>
                                                    </div>
                                                </div>

                                                <div class="detail-item row"
                                                     t-if="values['academic_data'] || values['residential_data']">
                                                    <div class="col-7">
                                                        <span class="label1">Student rating:</span>
                                                    </div>
                                                    <div class="col-5 ml-0 mr-0 p-0 text-nowrap">
                                                        <span class="label1 font-weight-normal">: </span>

                                                        <span class="number-value"
                                                              t-esc="values['academic_data']['student_feedback_rating']"/>
                                                        <span class="number-value"
                                                              t-esc="values['residential_data']['student_feedback_rating']"/>
                                                        <span class="number-value text-primary">/ 5</span>
                                                    </div>
                                                </div>
                                                <div class="detail-item row" t-if="values['common_performance']">
                                                    <div class="col-7">
                                                        <span class="label1">Qualitative rating:</span>
                                                    </div>
                                                    <div class="col-5 ml-0 mr-0 p-0 text-nowrap">
                                                        <span class="label1 font-weight-normal">: </span>

                                                        <span class="number-value"
                                                              t-esc="values['common_performance']['qualitative_average_rating']"/>
                                                        <span class="number-value text-primary">/ 5</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col m-2">
                            <div class="container-fluid mt-3">
                                <div class="row justify-content-center">
                                    <div class="col-md-10" style="width:100%">
                                        <div class="linechart-canvas-container" style="position:relative;height:400px;">
                                            <canvas id="LineChart1" style="width:600px;height:350px;"></canvas>
                                        </div>
                                    </div>

                                    <div class="col-xs-1 inline-block">
                                        <div style="margin-left: auto; margin-right: 0;">
                                            <span class="font-weight-bold">Year: </span>
                                            <span class="filter_year font-weight-bold" t-esc="values['year']"/>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

